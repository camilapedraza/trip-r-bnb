<%= simple_form_for(@car) do |f| %>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :location,
  input_html: {data: {location_autocomplete_target: "location"}},
  wrapper_html: {data: {controller: "location-autocomplete", location_autocomplete_api_key_value: ENV["MAPBOX_API_KEY"]}}
%>
  <%= f.input :price %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.submit %>
<% end %>
<a><%= link_to 'Back to cars list', cars_path %></a>

<div style="width: 100%; height: 600px;"
  data-controller="map"
  data-map-markers-value="<%= @markers.to_json %>"
  data-map-api-key-value="<%= ENV['MAPBOX_API_KEY'] %>">
</div>
<ul>
  <% @cars.each do |car| %>
    <li><%= link_to car.name, car_path(car) %></li>
  <% end %>
</ul>

<a><%= link_to 'Create a new car', new_car_path %></a>
